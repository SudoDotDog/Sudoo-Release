  },
  "homepage": "https://github.com/SudoDotDog/Sudoo-Release#readme",
  "dependencies": {
    "@sudoo/coco": "^1.8.5",
    "@sudoo/io": "^1.8.0"
  },
  "devDependencies": {
    "@sudoo/triforce": "^2.7.9"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
