  "description": ":8ball: Release helper for TS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SudoDotDog/Sudoo-Release.git"
  },
  "keywords": [
    "Release"
  ],
  "author": "WMXPY",
  "license": "MIT",
  "bugs": {
